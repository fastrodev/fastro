{
  "lock": false,
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "exports": {
    ".": "./mod.ts"
  },
  "imports": {
    "react": "npm:react@^19.2.4",
    "react/jsx-runtime": "npm:react@^19.2.4/jsx-runtime",
    "react/jsx-dev-runtime": "npm:react@^19.2.4/jsx-dev-runtime",
    "react-dom": "npm:react-dom@^19.2.4"
  },
  "compilerOptions": {
    "lib": [
      "deno.worker",
      "deno.unstable",
      "deno.ns",
      "deno.window",
      "dom"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": [
      "npm:@types/react@^19",
      "npm:@types/react-dom@^19"
    ]
  },
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run -A --watch=static/,routes/,core/ --env --unstable-kv main.ts 3000",
    "start": "ENV=production deno run -A --env --unstable-kv main.ts 3000"
  }
}
